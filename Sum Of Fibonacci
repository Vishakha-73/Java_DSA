package recursion;

public class fibonacci {
	    
	public static int fib(int x)
	        {
	            if(x<=1)
	            	return x;
	            
	           return fib(x-1)+fib(x-2);
	        }

	public static void main(String[] args)
	    {
		int sum=0;
	       int n=fib(10);
	       System.out.println(n);
	       
	       //sum of series till entered number
	       for(int i=0;i<10;i++) {
	    	    sum=sum+fib(i);
	       }
	       System.out.print("sum is: " + sum);
	    }
	}
